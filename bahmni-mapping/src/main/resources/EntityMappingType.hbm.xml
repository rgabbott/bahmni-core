<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.bahmnimapping.model">
    <class name="EntityMappingType" table="entity_mapping_type" batch-size="25">
        <id name="id" type="int"> <generator class="native" /> </id>
        <property name="name" type="java.lang.String"/>
        <property name="uuid" type="java.lang.String"/>
        <property name="entity1Type" type="java.lang.String" column="entity1_type"/>
        <property name="entity2Type" type="java.lang.String" column="entity2_type"/>
    </class>
</hibernate-mapping>